        Authentication authentication = new Authentication();
        // Create admin
        information adminInfo = new information("omar", "123");
        authentication.signupAdmin(adminInfo);

        // Login Admin
        information adminInfoTest = new information("oar", "123");
        Admin admin = authentication.loginAdmin(adminInfoTest);


        System.out.println(admin);


